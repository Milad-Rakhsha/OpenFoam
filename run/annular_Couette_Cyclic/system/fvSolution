FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        tolerance       1e-12;
        relTol          0.0;
        smoother        GaussSeidel;
    }

    pFinal
    {
        $p;
        relTol          0;
    }

    U
    {
        solver          PBiCG;
        smoother        symGaussSeidel;
        tolerance       1e-11;
        relTol          0;
        maxIter         2000;
        preconditioner  DILU;
    }
}

PISO
{
    nCorrectors     12;
    nNonOrthogonalCorrectors 0;
}


